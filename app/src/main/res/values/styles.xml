<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="StyledCheckbox" parent="@style/Widget.AppCompat.CompoundButton.CheckBox">
        <item name="android:colorControlNormal">@color/color1</item>
        <item name="android:colorControlActivated">@color/color2</item>
        <item name="android:colorControlHighlight">@color/color3</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat</item>
        <item name="android:text">@style/TextAppearance.AppCompat</item>
    </style>

</resources>
